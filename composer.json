{
    "autoload": {
        "psr-4": {
            "Butler\\Audit\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Butler\\Audit\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "description": "Glesys Butler Audit package.",
    "extra": {
        "laravel": {
            "providers": [
                "Butler\\Audit\\ServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "name": "glesys/butler-audit",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/bus": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/log": "^8.0",
        "illuminate/queue": "^8.0",
        "illuminate/support": "^8.23"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.5",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.5",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "type": "library"
}
